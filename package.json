{
  "name": "NB1",
  "version": "2.1.1",
  "description": "A tool that redfines your web experience, making complex online tasks way easier",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:lib": "rollup --config rollup.lib.config.js && cp package.lib.json dist-lib/package.json",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "exit 0",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.2",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "accname": "^1.1.0",
    "construct-style-sheets-polyfill": "3.1.0",
    "dotenv": "^16.4.7",
    "formik": "^2.4.6",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "openai": "^4.85.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-textarea-autosize": "^8.5.7",
    "react-use": "^17.6.0",
    "tailwindcss": "^3.4.17",
    "webextension-polyfill": "0.10.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "18.1.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "11.1.6",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.251",
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/jest": "29.5.7",
    "@types/lodash": "^4.17.15",
    "@types/node": "20.11.24",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.22",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.20",
    "chokidar": "3.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.7",
    "npm-run-all": "4.1.5",
    "postcss": "^8.5.2",
    "prettier": "3.2.5",
    "rollup": "4.17.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.2.0",
    "sass": "1.72.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "tslib": "2.6.2",
    "typescript": "5.5.3",
    "vite": "5.4.0",
    "ws": "8.18.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@8.9.0"
}
